// 아래 기능 비활성화
// GitHub Copilot 제안
// 인라인 코드 제안
// 자동 완성 제안 (일반, 주석, 문자열)
// 트리거 문자 제안
// 단어 기반 제안
// 파라미터 힌트
{
    "github.copilot.enable": {
        "*": false
    },
    "editor.inlineSuggest.enabled": false,
    "editor.quickSuggestions": {
        "other": false,
        "comments": false,
        "strings": false
    },
    "editor.suggestOnTriggerCharacters": false,
    "editor.wordBasedSuggestions": "off",
    "editor.parameterHints.enabled": false
}